{
  "update.mode": "manual",
  "extensions.autoUpdate": true,
  "extensions.autoCheckUpdates": true,
  "extensions.ignoreRecommendations": true,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "editor.rulers": [
    80
  ],
  "editor.minimap.maxColumn": 80,
  "editor.minimap.showSlider": "always",
  "editor.formatOnSave": false,
  "editor.renderWhitespace": "all",
  "editor.wordWrap": "wordWrapColumn",
  "editor.wrappingIndent": "indent",
  "workbench.editor.enablePreview": false,
  "workbench.editor.showTabs": false,
  "workbench.colorTheme": "Default High Contrast",
  "workbench.colorCustomizations": {
    "editorBracketHighlight.foreground1": "#ffffff",
    "editorBracketHighlight.foreground2": "#68aeff",
    "editorBracketHighlight.foreground3": "#5de1ff",
    "editorBracketHighlight.foreground4": "#5bffa5"
  },
  "explorer.compactFolders": false,
  "explorer.autoReveal": false,
  "explorer.confirmDragAndDrop": false,
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": false
  },
  "files.associations": {
    "*.glh": "glsl",
    "*.glf": "glsl",
    "*.glv": "glsl",
    "*.gl*": "glsl",
    "cmath": "cpp",
    "filesystem": "cpp",
    "memory": "cpp",
    "xlocbuf": "cpp",
    "xlocnum": "cpp",
    "algorithm": "cpp",
    "atomic": "cpp",
    "cctype": "cpp",
    "chrono": "cpp",
    "compare": "cpp",
    "concepts": "cpp",
    "csignal": "cpp",
    "cstddef": "cpp",
    "cstdint": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "cstring": "cpp",
    "ctime": "cpp",
    "cwchar": "cpp",
    "exception": "cpp",
    "fstream": "cpp",
    "functional": "cpp",
    "initializer_list": "cpp",
    "iomanip": "cpp",
    "ios": "cpp",
    "iosfwd": "cpp",
    "iostream": "cpp",
    "istream": "cpp",
    "iterator": "cpp",
    "limits": "cpp",
    "list": "cpp",
    "locale": "cpp",
    "map": "cpp",
    "mutex": "cpp",
    "new": "cpp",
    "ostream": "cpp",
    "ratio": "cpp",
    "regex": "cpp",
    "set": "cpp",
    "sstream": "cpp",
    "stdexcept": "cpp",
    "streambuf": "cpp",
    "string": "cpp",
    "system_error": "cpp",
    "thread": "cpp",
    "tuple": "cpp",
    "type_traits": "cpp",
    "typeinfo": "cpp",
    "unordered_map": "cpp",
    "utility": "cpp",
    "vector": "cpp",
    "xfacet": "cpp",
    "xhash": "cpp",
    "xiosbase": "cpp",
    "xlocale": "cpp",
    "xlocinfo": "cpp",
    "xlocmes": "cpp",
    "xlocmon": "cpp",
    "xloctime": "cpp",
    "xmemory": "cpp",
    "xstddef": "cpp",
    "xstring": "cpp",
    "xtr1common": "cpp",
    "xtree": "cpp",
    "xutility": "cpp"
  },
  "git.requireGitUserConfig": false,
  // extensions
  "vim.hlsearch": true,
  "vim.searchHighlightColor": "#006000",
  "vim.searchHighlightTextColor": "#FFFFFF",
  "vim.vimrc.enable": true,
  "C_Cpp.errorSquiggles": "enabled",
  "C_Cpp.clang_format_style": "file",
  "C_Cpp.default.cStandard": "c11",
  "C_Cpp.default.cppStandard": "c++17",
  "C_Cpp.loggingLevel": "None",
  "hexeditor.columnWidth": 16,
  "hexeditor.showDecodedText": true,
  "hexeditor.defaultEndianness": "little",
  "hexeditor.inspectorType": "aside",
  "glassit.alpha": 220
}